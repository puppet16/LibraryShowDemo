<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.luck.libraryshowdemo">

    <application
        android:name=".base.MyApplication"
        android:allowBackup="true"
        android:appComponentFactory=""
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:appComponentFactory">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.ToastyActivity"
            android:theme="@style/TranslateTheme" />
        <activity
            android:name=".activity.SwitchIconActivity"
            android:theme="@style/TranslateTheme" />
        <activity
            android:name=".activity.StyleableToastActivity"
            android:theme="@style/TranslateTheme" />
        <activity
            android:name=".activity.PhoneRegexActivity"
            android:theme="@style/TranslateTheme" />
        <activity
            android:name=".activity.FlowLayoutActivity"
            android:theme="@style/TranslateTheme" />
        <activity
            android:name=".activity.ExcelPanelActivity"
            android:theme="@style/TranslateTheme" />
    </application>

</manifest>